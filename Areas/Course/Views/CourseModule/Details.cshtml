@model LMS.Models.Course.CourseModule
@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";



}

<br />
<h2 class="text-info">Details Category</h2>
<br />

<form method="post" asp-action="Details">

    <div class="border backgroundWhite">

        <div class="form-group row">

            <div class="col-2">
                <label asp-for="Title" class="col-form-label"></label>
            </div>

            <div class="col-5">
                <input asp-for="Title" readonly class="form-control" />
            </div>
            <span asp-validation-for="Title" class="text-danger"></span>
        </div>
        <div class="form-group row">
            <div class="col-5 offset-2">
                <partial name="_EditAndBackToListPartial" model="@Model.Id" />
            </div>
        </div>

    </div>

</form>



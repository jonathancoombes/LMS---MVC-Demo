@model  int

<td style="width:150px;">

    <div class="btn-group" role="group">

        <a class="btn btn-sm btn-edit text-white" asp-action="Edit" asp-route-Id="@Model">
            <i class="fas fa-edit"></i>
        </a>

        <a class="btn btn-sm btn-danger text-white" asp-action="Delete" asp-route-Id="@Model">
            <i class="fas fa-trash-alt"></i>
        </a>
    </div>



</td>